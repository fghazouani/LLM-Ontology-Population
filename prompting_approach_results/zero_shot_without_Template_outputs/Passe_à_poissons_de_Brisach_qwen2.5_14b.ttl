@prefix : <http://www.semanticweb.org/fethi/ontologies/2024/1/TetraOnto.owl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .


:Brisach_Pass a :reconnection, :restorationMeasure ;
    :includes :Brisach_FishPass,
			      :SurfaceEntry, 
			      :DeepEntry, 
			      :ClassicalFishPass, :DescentSystem, :Pitfall ;
    :hasBiologicalMonitoring "oui"^^xsd:string ;
    :hasHydromorphologicaMonitoring "non"^^xsd:string ;
    :isManagedBy :CERGASA_ProjectOwner ;
    :relizedIn :Brisach_GeographicZone ;
    :concerns :ContinuityLongitudinal .

:Brisach_FishPass a :fishLock, :technicalElement ;
    :includes :SurfaceEntry, :DeepEntry, :ClassicalFishPass, :DescentSystem, :Pitfall ;
    :isLocatedOn :Rhin_WaterBody ;
    :hasMainContractor :CERGASA_MainContractor .

:SurfaceEntry a :basinPasses .
:DeepEntry a :basinPasses .
:ClassicalFishPass a :fishLift .
:DescentSystem a :baffleBrush, :technicalElement .
:Pitfall a :trapPass, :technicalElement .

:CERGASA_ProjectOwner a :projectOwner .
:CERGASA_MainContractor a :mainContractor .

:Brisach_GeographicZone a :geographicZone .

:Rhin_WaterBody a :river .