@prefix : <http://www.semanticweb.org/fethi/ontologies/2024/1/TetraOnto.owl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:PasseAPoissonsDeRheinfelden a :successivesBasinsPass ;
                               :associatedTo :CentraleHydroélectriqueRheinfelden ;
                               :concerns :LongitudinalContinuity ;
                               skos:prefLabel "Passe à poissons de Rheinfelden" .

:CentraleHydroélectriqueRheinfelden a :dam ;
                                     skos:prefLabel "Nouvelle centrale de Rheinfelden" ;
                                     :isLocatedOn :RheinfeldenAllemagne ;
                                     :isImpassable "true"^^xsd:boolean .

:RheinfeldenAllemagne a :geographicZone ;
                       skos:prefLabel "Rheinfelden, Allemagne" .

:LeRhin a :river ;
        skos:prefLabel "Le Rhin" ;
        :isLocatedOn :DépartementdeBadeWurtemberg . 

:DépartementdeBadeWurtemberg a :geographicZone ;
                               skos:prefLabel "Département de Bade-Wurtemberg" .