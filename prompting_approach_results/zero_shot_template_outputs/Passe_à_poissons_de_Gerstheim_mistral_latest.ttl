@prefix : <http://www.semanticweb.org/fethi/ontologies/2024/1/TetraOnto.owl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

 :MesureDeRestaurationGerstheim a :reconnection ;
        :associatedTo :PasseAPoissonsGerstheim ;
        :isManagedBy :CERGASA ;
        :hasMainContractor :None ;
        :startsAt <"StartDateTime">^^xsd:dateTime;
        :endsAt <"EndDateTime">^^xsd:dateTime ;
        :includes :PasseNaturelle ;
        :numberOfPools 3 ;
        :numberOfBasins 0 ;
        :averageSlope <AverageSlope> ;
        skos:prefLabel "Gerstheim Fish Pass Restoration Measure" .

:PasseAPoissonsGerstheim a :baffleFishway, :structure ;
                :isLocatedOn :LeRhin ;
                :hasHeight <heightOfTheStructure> ;
                :isImpassable false ;
                skos:prefLabel "Gerstheim Fish Pass" .

:LeRhin a :river, :waterBody ;
        skos:prefLabel "The Rhine" .

:CERGASA a :projectOwner ;
        skos:prefLabel "CERGA SA" .

:None a :mainContractor ;
        skos:prefLabel "No Main Contractor" .

:PasseNaturelle a :TechnicalElement, :subClassOfBassinSuccessif ;
             skos:prefLabel "Natural Pass" .