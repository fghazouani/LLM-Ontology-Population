@prefix : <http://www.semanticweb.org/fethi/ontologies/2024/1/TetraOnto.owl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

:<RestorationMeasureInstance> a :ContinuitéLongitudinale ;
			:associatedTo <StructureInstance> ;
			:isManagedBy <ProjectOwnerInstance> ;
			:hasMainContractor <MainContractorInstance> ;
			:startsAt "2008"^^xsd:dateTime;
			:endsAt "ongoing"^^xsd:dateTime ;
			:hasMaxLowWaterFlow "theMaxLowWaterFlow"^^xsd:float ;
			:hasMinLowWaterFlow "theMinLowWaterFlow"^^xsd:float> ;
			:includes <TechnicalElementInstance> ;
			:numberOfPools 4 ;
			skos:prefLabel "Passe à poissons de Brisach".

:<StructureInstance> a :Fishway ;
                    :isLocatedOn <GeographicZoneInstance> ;
                    :hasHeight "5,7"^^xsd:decimal ;
                    skos:prefLabel "Barrage agricole".

:<GeographicZoneInstance> a :geographicZone ;
                          :associatedTo <WaterBodyInstance> ;
                          skos:prefLabel "Le Rhin".

:<WaterBodyInstance> a :river ;
                        :hasPoolLength "the pool length"^^xsd:decimal ;
                        skos:prefLabel "Le Rhin".

:<ProjectOwnerInstance> a :projectOwner ;
                        skos:prefLabel "CERGA SA".

:<MainContractorInstance> a :mainContractor ;
                          skos:prefLabel "CERGA SA".

:<TechnicalElementInstance> a :TechnicalElement ;
                            :type "Système de dévalaison" ;
                            skos:prefLabel "Passe à poissons".

:<BiologicalMonitoringInstance> a :BiologicalMonitoring ;
                                  :hasFrequency "the frequency of monitoring"^^xsd:string ;
                                  skos:prefLabel "Oui".

:<HydromorphologicaMonitoringInstance> a :HydromorphologicaMonitoring ;
                                        :hasFrequency "the frequency of monitoring"^^xsd:string ;
                                        skos:prefLabel "Non".