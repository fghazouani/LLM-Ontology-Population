
@prefix : <http://www.semanticweb.org/fethi/ontologies/2024/1/TetraOnto.owl#> .
@prefix xs: <http://www.w3.org/2001/XMLSchema#> .

:PoissonPassStrasbourg a :fishway ;
    skos:prefLabel "Passe à Poissons de Strasbourg" ;
    geog:hasCategory :LongitudinalContinuityRestoration ;
    isManagedBy :EDFElectriciteDeFrance ;
    hasMainContractor :CentreDEngenieHydrauliqueEdfRD ;
    includes :FishWayEntryA ,
             :LocalAutomatedCounting ,
             :PublicObservationSpace,
             :ConcreteBasinSeriesABas , 
             :NaturalizedRiverB ,
             :ConcreteBasinSeriesBBas ; 
    startsAt "2012-01-01"^^xs:gYear ;
    endsAt   "2016-12-31"^^xs:gYear;
    costsTotalAmount "€15MILLION" ;
    isFinancedToTheExtentOf "30%"^ ^xsd:string ;
     by :RheinMeuseWaterAgency.

:Rhine a geo:SpatialObject ;
       skos:prefLabel "Le Rhin" .

:Strasbourg a geogeo:SpatialZone ;
            skos:prefLabel "Strasbourg" . 

:BassRhin a geo:SpatialZone; 
           skos:prefLabel "le Bas-rhin".

:FishWayEntryA a :entryExitPoint ;
               skos:prefLabel "Entrée principale du passage piscicole A". 

:LocalAutomatedCounting a :monitoringFacility ;
                       skos:prefLabel "Local de Comptage Automatise" . 

:PublicObservationSpace a :receptionAreaForVisitors ;
                        skos:prefLabel "espace d'observation publique".
                        
:ConcreteBasinSeriesABas a :seriesOfPool ;
                         skos:prefLabel "Série de Bassins en Béton ABAS";
                          hasNumberOfElements "18"^^xsd:integer.
                         
:NaturalizedRiverB a :naturalizationChannel ;
                  skos:prefLabel "rivière Pseudo-Naturelle B";  
                   hasLengthInMeter "500"^^xsd:decimal;
                    widthInMeter "5.5